= form_for @social_media_image do |f|
  - if @social_media_image.errors.any?
    #error_explanation
      %h2= "#{pluralize(@social_media_image.errors.count, "error")} prohibited this social_media_image from being saved:"
      %ul
        - @social_media_image.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :socials_need_id
    = f.number_field :socials_need_id
  .field
    = f.label :image
    = f.text_field :image
  .actions
    = f.submit 'Save'
