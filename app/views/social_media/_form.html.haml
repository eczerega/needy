= form_for @social_medium do |f|
  - if @social_medium.errors.any?
    #error_explanation
      %h2= "#{pluralize(@social_medium.errors.count, "error")} prohibited this social_medium from being saved:"
      %ul
        - @social_medium.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :social_need_id
    = f.text_field :social_need_id
  .field
    = f.label :integer
    = f.text_field :integer
  .field
    = f.label :content_url
    = f.text_field :content_url
  .field
    = f.label :string
    = f.text_field :string
  .field
    = f.label :content_type
    = f.text_field :content_type
  .field
    = f.label :integer
    = f.text_field :integer
  .actions
    = f.submit 'Save'
