= form_for @challenge_solution do |f|
  - if @challenge_solution.errors.any?
    #error_explanation
      %h2= "#{pluralize(@challenge_solution.errors.count, "error")} prohibited this challenge_solution from being saved:"
      %ul
        - @challenge_solution.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.text_field :user_id
  .field
    = f.label :integer
    = f.text_field :integer
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :string
    = f.text_field :string
  .field
    = f.label :challenge_id
    = f.text_field :challenge_id
  .field
    = f.label :integer
    = f.text_field :integer
  .field
    = f.label :destription
    = f.text_field :destription
